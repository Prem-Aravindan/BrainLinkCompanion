    "buffer": "^6.0.3",
    "expo": "53.0.19",
    "expo-asset": "^11.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-ble-plx": "^3.5.0",
